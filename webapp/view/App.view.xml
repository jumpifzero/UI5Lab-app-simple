<mvc:View
    controllerName="ui5lab.app.SquareApp.controller.App"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
  <App>
    <pages>
      <Page title="{i18n>title}">
        <content>
          <List
			  headerText="Entries"
			  items="{/Items}" >
			<FeedListItem
				sender="{Author}"
				icon="{AuthorPicUrl}"
				senderPress="onPress"
				iconPress="onPress"
				iconDensityAware="false"
				info="{Type}"
				timestamp="{Date}"
				text="{Text}"
				convertLinksToAnchorTags="All" />
		  </List>
        </content>
      </Page>
    </pages>
  </App>
</mvc:View>
